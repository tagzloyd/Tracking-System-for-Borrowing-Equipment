{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "@vercel/php"
    },
    {
      "src": "public/build/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)/Attendance/.*",
      "dest": "api/index.php"
    },
    {
      "src": "/(.*)",
      "dest": "public/build/index.html"
    }
  ]
}